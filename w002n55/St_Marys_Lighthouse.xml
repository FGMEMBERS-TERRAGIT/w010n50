<?xml version="1.0"?>

<PropertyList>

 <path>St_Marys_Lighthouse.ac</path>

 <animation>
  <type>range</type>
  <min-m>0</min-m>
  <max-m>16000</max-m>
 </animation>
 
 <animation>
  <type>alpha-test</type>
  <alpha-factor>0.01</alpha-factor>
 </animation>

<animation>
    <type>rotate</type>
<object-name>WV</object-name>
    <property>/environment/wind-from-heading-deg</property>
    <factor>-1</factor>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>1</z>
    </axis>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center>
  </animation>

<animation>
  <type>rotate</type>
  <object-name>SeaGull</object-name>
<property>/sim/time/elapsed-sec</property>
  <factor>15</factor>
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>0</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>0</y>
   <z>-1</z>
  </axis>
 </animation>

<animation>
  <type>rotate</type>
  <object-name>SeaGull2</object-name>
<property>/sim/time/elapsed-sec</property>
  <factor>15</factor>
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>0</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>0</y>
   <z>1</z>
  </axis>
 </animation>

</PropertyList>
