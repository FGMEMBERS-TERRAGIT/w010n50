
<PropertyList>

 <path>superhangar.ac</path>
 
 <sound>
  <path>Scenery/EGOD/Models/superhangar-sound.xml</path>
 </sound>
 
 <nasal>
  <load>
   if (!contains(globals, "scenery")) globals["scenery"] = {};
   if (!contains(scenery, "EGOD")) scenery["EGOD"] = {};
   if (!contains(scenery.EGOD, "hangar")) scenery.EGOD["hangar"] = {};
   if (contains(scenery, "sharedDoor")) {
     scenery.EGOD.hangar[0] =
       { door :
         [scenery.sharedDoor.new("/scenery/EGOD/hangar[0]/door[0]", 30),
          scenery.sharedDoor.new("/scenery/EGOD/hangar[0]/door[1]", 30)
         ]
       };
   } else {
     scenery.EGOD.hangar[0] =
       { door :
         [aircraft.door.new("/scenery/EGOD/hangar[0]/door[0]", 30),
          aircraft.door.new("/scenery/EGOD/hangar[0]/door[1]", 30)
         ]
       };
   }
  </load>
  <unload>
   if (contains(scenery, "sharedDoor")) {
     scenery.EGOD.hangar[0].door[0].destroy();
     scenery.EGOD.hangar[0].door[1].destroy();
   }
  </unload>
 </nasal>

 <animation>
  <type>pick</type>
   <object-name>Door.002</object-name>
   <object-name>Door.003</object-name>
   <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <interval-sec>0.2</interval-sec>
    <binding>
     <command>nasal</command>
	 <script>
	  scenery.EGOD.hangar[0].door[0].toggle();
     </script>
    </binding>
   </action>
 </animation>
 
