<?xml version="1.0"?>

<PropertyList>

 <path>BoL_Buildings.ac</path>

 <animation>
  <type>range</type>
  <min-m>0</min-m>
  <max-m>16000</max-m>
 </animation>

 <animation>
  <type>alpha-test</type>
  <alpha-factor>0.01</alpha-factor>
 </animation>

<animation>
  <type>rotate</type>
  <object-name>SeaGull</object-name>
<property>/sim/time/elapsed-sec</property>
  <factor>15</factor>
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>0</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>-4</z>
  </axis>
 </animation>

</PropertyList>
